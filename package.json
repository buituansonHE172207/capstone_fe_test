{
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "preview": "vite preview",
    "lint": "eslint .",
    "build": "vite build  && npm run server:build",
    "server": "node --loader ts-node/esm ./server/index.ts",
    "server:dev": "npm run server",
    "server:prod": "node ./build/index.cjs",
    "server:prod:ts": "cross-env NODE_ENV=production npm run server",
    "server:build": "tsx scripts/buildServer/buildServer",
    "test:lint": "npm run test:lint:eslint",
    "test:lint:eslint": "eslint .",
    "test:lint:locales": "scripts/locales/locales.sh src/locales",
    "test:lint:remark": "remark . --quiet --frail",
    "test:lint:style": "stylelint --max-warnings 0 --ignore-path .gitignore \"**/*.{css,scss}\"",
    "test:unit": "npm run test:unit:dev -- run --coverage",
    "test:unit:update": "npm run test:unit:dev -- run --coverage -u",
    "test:unit:dev": "vitest",
    "test": "npm run test:lint && npm run test:unit",
    "update": "npx npm-check-updates",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/react-query": "^5.52.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "antd": "^5.20.2",
    "axios": "^1.7.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "js-cookie": "^3.0.5",
    "node-fetch": "^3.3.2",
    "part-regex": "^0.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-streaming": "^0.3.14",
    "react-toastify": "^10.0.5",
    "sirv": "^2.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vike": "^0.4.184",
    "vike-react-query": "^0.1.1",
    "vite": "^5.4.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "type": "module",
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tanstack/react-query-devtools": "^5.8.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/fs-extra": "^11.0.4",
    "autoprefixer": "^10.4.20",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.17.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0", 
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0", 
    "eslint": "^8.57.0", 
    "eslint-config-standard": "^17.1.0", 
    "eslint-import-resolver-typescript": "^3.6.1", 
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1", 
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react-hooks": "^4.6.0", 
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-yml": "^1.14.0",
    "prettier": "^3.3.2"
  },
  "imports": {
    "#components/*": "./src/components/*",
    "#hooks/*": "./src/hooks/*",
    "#pages/*": "./src/pages/*",
    "#locales/*": "./src/locales/*",
    "#assets/*": "./src/assets/*",
    "#layouts/*": "./src/layouts/*",
    "#utils/*": "./src/utils/*",
    "#stores/*": "./src/stores/*",
    "#renderer/*": "./src/renderer/*",
    "#src/*": "./src/*",
    "#plugins/*": "./src/plugin/*",
    "#types/*": "./src/types/*",
    "#root/*": "./*"
  },
  "lint-staged": {
    "*.ts,": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  }
}
